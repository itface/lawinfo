<body class="login2">
<!-- Login Screen -->
<div class="login-wrapper">
    <form id="loginForm">
        <div class="form-group has-error">
            <label class="col-sm-10 control-label error-label" for="inputError">
            </label>
        </div>
        <div class="form-group">
            <div class="input-group">
                <span class="input-group-addon"><span class="glyphicon glyphicon-user"></span></span><input class="form-control" placeholder="手机号" type="text" name="tel" autocomplete="on">
            </div>
        </div>
        <div class="form-group">
            <div class="input-group">
                <span class="input-group-addon"><span class="glyphicon glyphicon-lock"></span></span><input class="form-control" name="code" placeholder="密码" type="text"><span class="input-group-addon"><button type="button" class="btn btn-info btn-sm" id="authCodeBtn">获取验证码</button></span>
            </div>
        </div>
        <input type="hidden" id="wechatopenid" name="wechatopenid" value="$!wechatopenid">
        <input type="hidden" id="wechatcode" name="wechatcode" value="$!wechatcode">
        <input class="btn btn-lg btn-default btn-block login" type="submit" value="登录">
    </form>
</div>
<div class="col-xs-12"  style="text-align: center;position: absolute;bottom:5px">明理金融法律服务团队@2015-2017 版权所有</div>
<script type="text/javascript">
    login.init();
</script>
</body>
